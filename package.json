{
  "name": "inter-intellisense",
  "description": "A vscode extension for Inter  HTML intellisense.",
  "module": "esnext",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.57.1"
  },
  "main": "./out/extension.js",
  "publisher": "Interjs org",
  "displayName": "Interjs HTML intellisense",
  "license": "MIT",
  "icon": "./images/icon.png",
  "activationEvents": [
    "onLanguage:html"
  ],
   "contributes": {
    "commands": [
      {
        "command": "inter.version",
        "title": "Inter version"
      }
    ],
	"html": {
		"customData": ["./customAttrs/html.html-data.json"]
		
	}

   },
  
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^5.4.5"
}
}
